{
  "name": "portpro-backend",
  "version": "1.0.0",
  "description": "A simple backend managing three different databases.",
  "main": "app.js",
  "engines": {
    "node": "18.x",
    "npm": "8.x"
  },
  "scripts": {
    "dev": "nodemon app.js",
    "seed:mysql": "npx sequelize-cli db:seed:all",
    "migrate:mysql": "npx sequelize-cli db:migrate",
    "migrate:mysql:undo": "npx sequelize-cli db:migrate:undo",
    "start": "node app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Prashant GM",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-cassandra": "^2.9.0",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "mongoose": "^7.0.3",
    "mysql2": "^3.2.0",
    "sequelize": "^6.30.0",
    "sequelize-cli": "^6.6.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
}
